+  courseId: PropTypes.string.isRequired,
+  metadataModel: PropTypes.string.isRequired,
+  streakLengthToCelebrate: PropTypes.number.isRequired,
+  intl: intlShape.isRequired,
+  open: PropTypes.bool,
+};
+
+export default injectIntl(StreakModal);
