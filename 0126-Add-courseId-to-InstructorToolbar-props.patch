Subject: [PATCH 0126/1021] Add courseId to InstructorToolbar props

---
 src/courseware/course/Course.jsx            | 1 +
 src/courseware/course/InstructorToolbar.jsx | 2 ++
 2 files changed, 3 insertions(+)

diff --git a/src/courseware/course/Course.jsx b/src/courseware/course/Course.jsx
index d294a04..a02bba8 100644
--- a/src/courseware/course/Course.jsx
+++ b/src/courseware/course/Course.jsx
@@ -72,6 +72,7 @@ function Course({
         />
         {isStaff && (
           <InstructorToolbar
+            courseId={courseId}
             unitId={unitId}
           />
         )}
diff --git a/src/courseware/course/InstructorToolbar.jsx b/src/courseware/course/InstructorToolbar.jsx
index c73462f..5b16071 100644
--- a/src/courseware/course/InstructorToolbar.jsx
+++ b/src/courseware/course/InstructorToolbar.jsx
@@ -39,11 +39,13 @@ function InstructorToolbar(props) {
 
 InstructorToolbar.propTypes = {
   activeUnitLmsWebUrl: PropTypes.string,
+  courseId: PropTypes.string,
   unitId: PropTypes.string,
 };
 
 InstructorToolbar.defaultProps = {
   activeUnitLmsWebUrl: undefined,
+  courseId: undefined,
   unitId: undefined,
 };
 
